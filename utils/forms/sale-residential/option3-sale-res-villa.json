                    "fieldName": "furnished_status"
                },
                {
                    "type": "option",
                    "label": "Bathrooms",
                    "initial_counts": [1,2,3,"3+"],
                    "more_counts": [4,5,6,7,8,9,10, ">10"],
                    "showMore": true,
                    "fieldName": "bathrooms"
                },
                {
                    "type": "dropdown",
                    "label": "No of open sides",
