                {
                    "type": "checkbox",
                    "label": "Response from brokers",
                    "fields": [
                        {
                            "label": "I am not interested in getting response from brokers",
                            "value": "I am not interested in getting response from brokers",
                            "fieldName": "response_from_brokers"
